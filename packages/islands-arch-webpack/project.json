{
  "name": "islands-arch-webpack",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/islands-arch-webpack/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/packages/islands-arch-webpack",
        "compiler": "babel",
        "main": "packages/islands-arch-webpack/src/main.ts",
        "tsConfig": "packages/islands-arch-webpack/tsconfig.app.json",
        "webpackConfig": "packages/islands-arch-webpack/webpack.config.js",
        "assets": [
          "packages/islands-arch-webpack/src/favicon.ico",
          "packages/islands-arch-webpack/src/assets"
        ],
        "index": "packages/islands-arch-webpack/src/index.html",
        "baseHref": "/",
        "styles": ["packages/islands-arch-webpack/src/styles.css"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "fileReplacements": [
            {
              "replace": "packages/islands-arch-webpack/src/environments/environment.ts",
              "with": "packages/islands-arch-webpack/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/webpack:dev-server",
      "options": {
        "buildTarget": "islands-arch-webpack:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "islands-arch-webpack:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/islands-arch-webpack/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/islands-arch-webpack/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  }
}
